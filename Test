SELECT * FROM  MDS.STAGING.T_MDS_ECM_VENDOR_DEF LIMIT 5
SELECT * FROM  MDS.STAGING.T_MDS_ECM_PLANT_TO_VENDOR_LOOKUP LIMIT 5
SELECT * FROM  MDS.STAGING.T_ECM_SOCIAL_COMPLIANCE_DUE_DILIGENCE LIMIT 5
SELECT * FROM  MDS.STAGING.T_MDS_ETQ_SUPPLIER_SITE_PROFILE_DEF LIMIT 5

create or replace TABLE MDS.STAGING.T_MDS_ECM_VENDOR_DEF (
	SAP_VENDOR_ID VARCHAR(16777216),
	VENDOR_NAME VARCHAR(16777216),
	CODE VARCHAR(16777216),
	SAP_VENDOR_NAME_ID VARCHAR(16777216),
	ACTIVE_VENDOR VARCHAR(16777216),
	REGION_CODE VARCHAR(16777216),
	PRIMARY_MFG_REGION VARCHAR(16777216),
	PRIMARY_MFG_COUNTRY VARCHAR(16777216),
	COMPANY_PROFILE VARCHAR(16777216),
	ETQ_COMPANY_PROFILE_NAME VARCHAR(16777216),
	REPRESENTATIVE_BUSINESS_SEGMENT VARCHAR(16777216),
	BUSINESS_UNIT VARCHAR(16777216),
	REPRESENTATIVE_PRODUCT_SECTOR VARCHAR(16777216),
	PRODUCT_CATEGORY VARCHAR(16777216),
	PRODUCT_EXAMPLES VARCHAR(16777216),
	MODEL VARCHAR(16777216),
	SEGMENTATION_CLASS VARCHAR(16777216),
	WGI_SCORE VARCHAR(16777216),
	PROCUREMENT_LEAD VARCHAR(16777216),
	MFG_LEAD VARCHAR(16777216),
	ECM_TEAM_COMMENTS VARCHAR(16777216),
	AUDIT_SOURCEFILENAME VARCHAR(16777216),
	AUDIT_INSERTED_TIMESTAMP TIMESTAMP_LTZ(9),
	AUDIT_UPDATED_TIMESTAMP TIMESTAMP_LTZ(9)
);


create or replace TABLE MDS.STAGING.T_MDS_ECM_PLANT_TO_VENDOR_LOOKUP (
	SAP_PLANT_NAME VARCHAR(16777216),
	SAP_PLANT_ID FLOAT,
	SAP_VENDOR_ID_NAME VARCHAR(16777216),
	AUDIT_SOURCEFILENAME VARCHAR(16777216),
	AUDIT_INSERTED_TIMESTAMP TIMESTAMP_LTZ(9),
	AUDIT_UPDATED_TIMESTAMP TIMESTAMP_LTZ(9)
);


create or replace TABLE MDS.STAGING.T_ECM_SOCIAL_COMPLIANCE_DUE_DILIGENCE (
	EVENT_ID VARCHAR(16777216),
	ETQ_SSP_NAME_ID VARCHAR(16777216),
	AUDIT_RENEWAL_DATE TIMESTAMP_NTZ(9),
	SUB_EVENT_SEQUENCE_NUMBER FLOAT,
	SUB_EVENT_TYPE VARCHAR(16777216),
	AUDITOR VARCHAR(16777216),
	ASSESSMENT_PROTOCOL VARCHAR(16777216),
	DUE_DILIGENCE_REQUESTER VARCHAR(16777216),
	DUE_DILIGENCE_TYPE VARCHAR(16777216),
	AUDIT_RENEWAL_EXECUTION_STATUS VARCHAR(16777216),
	DUE_DILIGENCE_REQUESTED_DATE TIMESTAMP_NTZ(9),
	CUSTOMER_DEADLINE_DATE TIMESTAMP_NTZ(9),
	DUE_DILIGENCE_DATE TIMESTAMP_NTZ(9),
	FINAL_DD_COMPLETION_DATE TIMESTAMP_NTZ(9),
	EVENT_OUTCOME_COMPLIANCY_STATUS VARCHAR(16777216),
	EVENT_OUTCOME_ACTION_REQUIRED VARCHAR(16777216),
	CAP_RECEIVED_INDICATOR VARCHAR(16777216),
	EXECUTION_REQUIRES_AUDIT_RESULTS_INDICATOR VARCHAR(16777216),
	MUTUAL_RECOGNITION_INDICATOR VARCHAR(16777216),
	ZERO_TOLERANCE_FINDINGS_COUNT FLOAT,
	CRITICAL_FINDINGS_COUNT FLOAT,
	MAJOR_1_FINDINGS_COUNT FLOAT,
	MINOR_FINDINGS_COUNT FLOAT,
	SECONDARY_MINOR_FINDINGS_COUNT FLOAT,
	HEALTH_SAFETY_FINDINGS_COUNT FLOAT,
	CHILD_LABOR_FINDINGS_COUNT FLOAT,
	FORCED_LABOR_FINDINGS_COUNT FLOAT,
	DISCRIMINATION_FINDINGS_COUNT FLOAT,
	DISC_HARR_ABUSE_FINDINGS_COUNT FLOAT,
	FREEDOM_OF_ASSN_FINDINGS_COUNT FLOAT,
	EMP_CONTRACTS_FINDINGS_COUNT FLOAT,
	WORKING_HOURS_FINDINGS_COUNT FLOAT,
	WAGES_BENEFITS_FINDINGS_COUNT FLOAT,
	DORMS_CANTEEN_FINDINGS_COUNT FLOAT,
	ENVIRONMENT_FINDINGS_COUNT FLOAT,
	OTHER_FINDINGS_COUNT FLOAT,
	TOTAL_MAJOR_FINDINGS_COUNT FLOAT,
	TOTAL_MINOR_FINDINGS_COUNT FLOAT,
	SOCIAL_COMPLIANCE_TEAM_COMMENTS VARCHAR(16777216),
	AUDIT_SOURCEFILENAME VARCHAR(16777216),
	AUDIT_INSERTED_TIMESTAMP TIMESTAMP_LTZ(9),
	AUDIT_UPDATED_TIMESTAMP TIMESTAMP_LTZ(9)
);


create or replace TABLE MDS.STAGING.T_MDS_ETQ_SUPPLIER_SITE_PROFILE_DEF (
	ETQ_SSP_ID VARCHAR(16777216),
	ETQ_SSP_NAME_ID VARCHAR(16777216),
	SAP_VENDORNAME_ID VARCHAR(16777216),
	ETQ_APPROVAL_STATUS VARCHAR(16777216),
	SOCIAL_COMPLIANCE_SITE_STATUS VARCHAR(16777216),
	SUPPLIER_TYPE VARCHAR(16777216),
	SUPPLIER_TYPE_BREAKDOWN VARCHAR(16777216),
	EIQ_RISK_SCORE FLOAT,
	SITE_OVERALL_RISK VARCHAR(16777216),
	DUE_DILIGENCE_FREQUENCY VARCHAR(16777216),
	EMPLOYEE_COUNT FLOAT,
	SOCIAL_COMPLIANCE_TEAM_COMMENTS VARCHAR(16777216),
	AUDIT_SOURCEFILENAME VARCHAR(16777216),
	AUDIT_INSERTED_TIMESTAMP TIMESTAMP_LTZ(9),
	AUDIT_UPDATED_TIMESTAMP TIMESTAMP_LTZ(9)
);
